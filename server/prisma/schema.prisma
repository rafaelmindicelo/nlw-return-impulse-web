generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postegresql"
  url      = env("DATABASE_URL")
}

model Feedback {
  // por boas práticas não se utiliza id incremental, por questões de segurança
  id String @id @default(uuid()) //@id indica primary key

  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}
